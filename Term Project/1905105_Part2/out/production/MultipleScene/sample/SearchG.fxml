<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" style="-fx-background-color: rgb(165,165,165);" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SearchGlobal.SearchPlayerGlobal">
    <children>
        <AnchorPane prefHeight="254.0" prefWidth="250.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="player" fitHeight="208.0" fitWidth="250.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
                <Label layoutX="14.0" layoutY="208.0" prefHeight="38.0" prefWidth="175.0" text="By Player Name">
                    <font>
                        <Font name="Agency FB Bold" size="30.0" />
                    </font>
                </Label>
                <Button fx:id="ByName" layoutY="1.0" mnemonicParsing="false" onAction="#SearchName" opacity="0.0" prefHeight="250.0" prefWidth="250.0" />
            </children>
        </AnchorPane>
        <AnchorPane layoutY="255.0" prefHeight="250.0" prefWidth="250.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="salary" fitHeight="208.0" fitWidth="250.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
                <Label layoutX="14.0" layoutY="208.0" prefHeight="38.0" prefWidth="175.0" text="By Salary Range">
                    <font>
                        <Font name="Agency FB Bold" size="30.0" />
                    </font>
                </Label>
                <Button fx:id="BySalary" layoutY="1.0" mnemonicParsing="false" onAction="#SearchSalary" opacity="0.0" prefHeight="250.0" prefWidth="250.0" />
            </children>
        </AnchorPane>
        <AnchorPane layoutX="252.0" layoutY="-1.0" prefHeight="254.0" prefWidth="250.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="country" fitHeight="208.0" fitWidth="250.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
                <Label layoutX="14.0" layoutY="208.0" prefHeight="38.0" prefWidth="186.0" text="By Country Name">
                    <font>
                        <Font name="Agency FB Bold" size="30.0" />
                    </font>
                </Label>
                <Button fx:id="ByCountry" layoutY="1.0" mnemonicParsing="false" onAction="#SearchCountry" opacity="0.0" prefHeight="250.0" prefWidth="250.0" />
            </children>
        </AnchorPane>
        <AnchorPane layoutX="252.0" layoutY="254.0" prefHeight="254.0" prefWidth="253.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="list" fitHeight="208.0" fitWidth="253.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
                <Label layoutX="14.0" layoutY="208.0" prefHeight="38.0" prefWidth="186.0" text="Country-wise list">
                    <font>
                        <Font name="Agency FB Bold" size="30.0" />
                    </font>
                </Label>
                <Button fx:id="Country_list" layoutY="1.0" mnemonicParsing="false" onAction="#CountryList" opacity="0.0" prefHeight="250.0" prefWidth="250.0" />
            </children>
        </AnchorPane>
        <AnchorPane layoutX="504.0" prefHeight="254.0" prefWidth="250.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="position" fitHeight="208.0" fitWidth="258.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
                <Label layoutX="14.0" layoutY="208.0" prefHeight="31.0" prefWidth="146.0" text="By Position">
                    <font>
                        <Font name="Agency FB Bold" size="30.0" />
                    </font>
                </Label>
                <Button fx:id="ByPosition" layoutX="2.0" layoutY="1.0" mnemonicParsing="false" onAction="#SearchPosition" opacity="0.0" prefHeight="250.0" prefWidth="254.0" />
            </children>
        </AnchorPane>
        <AnchorPane layoutX="505.0" layoutY="253.0" prefHeight="254.0" prefWidth="253.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="exit" fitHeight="208.0" fitWidth="254.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
                <Label layoutX="14.0" layoutY="208.0" prefHeight="31.0" prefWidth="146.0" text="Exit">
                    <font>
                        <Font name="Agency FB Bold" size="30.0" />
                    </font>
                </Label>
                <Button fx:id="Exit" layoutX="5.0" layoutY="2.0" mnemonicParsing="false" onAction="#Exit" opacity="0.0" prefHeight="250.0" prefWidth="250.0" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
